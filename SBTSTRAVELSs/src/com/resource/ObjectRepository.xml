<?xml version="1.0"?>
<pageElements>

	<page name="LoginPage">
		<element name="userName" Descriptor="id" value="email" />
		<element name="password" Descriptor="XPATH" value=".//*[@id='password']" />
		<element name="signInSubmit" Descriptor="id" value="login_submit" />
	</page>
	
	<page name="HomePage">
		<element name="signin" Descriptor="XPATH" value="html/body/div[1]/header/div[3]/div/div/div[2]/div[2]/div[2]/a" />
		<element name="title" Descriptor="tagName" value="Welcome - SBTS Travels" />
	</page>


<page name="RegisterPage">
      <element name="signin" Descriptor="XPATH" value="html/body/div[1]/header/div[3]/div/div/div[2]/div[2]/div[2]/a" />
		<element name="singup" Descriptor="XPATH" value=".//*[@id='login']/div[8]/a" />
		<element name="name" Descriptor="XPATH" value="//input[@name='first_name']" />
		<element name="mobile" Descriptor="XPATH" value="//input[@class='form-control logpadding']" />
		<element name="email" Descriptor="XPATH" value="//input[@placeholder='Email']" />
		<element name="password" Descriptor="XPATH" value=".//*[@id='register_user_form']/div[4]/input" />
		<element name="Retypepassword" Descriptor="XPATH" value=".//*[@id='register_user_form']/div[5]/input" />
		<element name="terms" Descriptor="XPATH" value=".//*[@id='register_user_form']/div[7]/div[1]/div/div/label" />
		<element name="RegisterButton" Descriptor="XPATH" value=".//*[@id='register_user_button']" />
		
	</page>

</pageElements>